# LLM 配置（通义千问 DashScope）
LLM_API_KEY=sk-xxx
LLM_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
LLM_MODEL=qwen-plus

# 邮件 SMTP 配置（阿里云请用 465 端口）
SMTP_HOST=smtp.qq.com
SMTP_PORT=465
SMTP_USER=your@qq.com
SMTP_PASSWORD=your_app_password
EMAIL_TO=your@email.com

# 定时任务（北京时间）
CRON_HOUR=9
CRON_MINUTE=0

# GitHub Token（可选，提高 API 限额）
GITHUB_TOKEN=

# 抓取配置
TRENDING_LANGUAGES=python,javascript,typescript,vue,go,rust
TRENDING_SINCE=daily
MAX_PROJECTS=25

# 个性化技术栈（用于 AI 加权评分）
USER_TECH_STACK=Vue.js,React,FastAPI,Three.js,Cesium,Docker,n8n,Dify,AI/LLM

# 去重天数
DEDUP_DAYS=7
